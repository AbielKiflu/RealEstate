@model IEnumerable<RealEstate.Models.Property>

 

@{
    ViewData["Title"] = "Index";
        
}

<h1>Rents</h1>

 <div class="cards">
 
@foreach (var item in Model) {
        
  <div class="card" style="width: 18rem;">
  
    <img  class="card-img-top" src="@("~/Images/"+ @item.Picture)" asp-append-version="true"/>

    <div class="card-body">
        <h5 class="card-title"> @Html.DisplayFor(modelItem => item.Description)</h5>
        <p class="card-text">

        </p>
        <a href="#" class="btn btn-primary">Go somewhere</a>
        </div>
    </div>

        }
 
        </div>